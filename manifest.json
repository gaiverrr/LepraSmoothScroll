{
  "name": "Lepra Smooth Scroll Extension",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Adds smooth scroll to top of the current page",
  "content_scripts": [ {
    "js": [ "scripts/jquery.js", "scripts/background.js" ],
    "css": ["styles/main.css"],
    "matches": [ "http://*.leprosorium.ru/*", "http://leprosorium.ru/*" ]
  } ],
  "icons": {
    "48": "images/icon-48.png"
  },
  "default_locale": "en"
}